<!doctype html>
<html lang="en">

	<head>
		<meta charset='utf-8'/>
		<title>Proto</title>

		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link rel="stylesheet" type="text/css" href="/applications/dashboard/design/style.css?v=2.1b1.17" media="all" />
		<link href="/styles.css" media="screen" rel="stylesheet" type="text/css" />
	</head>

	<body class="">
		<div class="proto">
			<div id="follow-bar">
				<div class="container">
					<div class="teamspeak pull-left">
						<img src="//www.teamspeak.com/media_repository/files/mediakit/buttons/button01_88x31.gif" />
						<div></div>
					</div>
					<div class="pull-right">
						Follow us on
						<img src="//www.facebookbrand.com/img/assets/asset.f.logo.lg.png" />
						<img src="//twitter.com/images/resources/twitter-bird-white-on-blue.png" />
					</div>
				</div>
			</div>
			<div id="navbar" class="navbar navbar-fixed-top">
				<div class="navbar-inner">
					<div class="container">
						<a class="brand" href="/">Proto</a>
						<div class="nav-collapse collapse">
							<ul class="nav" id="main-menu">
								<li class="active"><a href="/">Home</a></li>
								{{! Use a "." relative path to defeat push state. }}
								<li><a href="./discussions" class="forums-link">Forums</a></li>
							</ul>
							<img class="loading-indicator" src="/images/ajax-loader.gif" alt="Loading..." />
							<ul class="nav pull-right">
								<li class="divider-vertical"></li>
								<li>
								{{#if this._app.attributes.user.authenticated}}
									<a href="./entry/signout?TransientKey={{this._app.attributes.user.transientKey}}">
										Sign out
									</a>
								{{else}}
									<a href="./entry/signin" class="SignInPopup">Sign In</a>
								{{/if}}
								</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
	
			<section id="content" class="container">
				{{{body}}}
			</section>

			<footer id="footer" class="container">
				<hr />
				<p>&copy; Prototype Gamer 2013</p>
			</footer>
		</div>

		<script src="/mergedAssets.js"></script>
		<script>
		(function() {
			var App = window.App = new (require('app/app'))({{json appData}});
			App.bootstrapData({{json bootstrappedData}});
			App.start();
		})();
		</script>

	</body>

</html>
